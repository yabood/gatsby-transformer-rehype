{
    "name": "@yabood/gatsby-transformer-rehype",
    "description": "Gatsby transformer plugin for HTML using the Rehype library and ecosystem",
    "version": "2.0.1",
    "license": "MIT",
    "author": "styxlab",
    "homepage": "https://github.com/styxlab/",
    "main": "gatsby-node.js",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/yabood/gatsby-transformer-rehype"
    },
    "bugs": {
        "url": "https://github.com/yabood/gatsby-transformer-rehype"
    },
    "keywords": [
        "gatsby",
        "gatsby-plugin",
        "gatsby-transformer-plugin",
        "html",
        "rehype",
        "jamify",
        "jamify-transformer",
        "jamify-transformer-rehype"
    ],
    "publishConfig": {
        "registry":"https://npm.pkg.github.com"
    },
    "scripts": {
        "build": "babel src --out-dir . --ignore **/__tests__",
